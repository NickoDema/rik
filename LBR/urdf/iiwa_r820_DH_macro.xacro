<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">
  <xacro:macro name="iiwa_r820" params="prefix">

    <!-- link list -->
    <link name="${prefix}iiwa_base_link">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/base_link.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/base_link.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l1">
      <visual>
        <origin xyz="0 -0.36 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_1.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 -0.36 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_1.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l2">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_2.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_2.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l3">
      <visual>
        <origin xyz="0 -0.42 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_3.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 -0.42 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_3.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l4">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_4.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_4.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l5">
      <visual>
        <origin xyz="0 -0.4 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_5.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 -0.4 0" rpy="-1.5708 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_5.stl" />
        </geometry>
      </collision>
    </link>

    <link name="${prefix}iiwa_l6">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_6.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_6.stl" />
        </geometry>
      </collision>
    </link>

    <!-- iiwa_l7 -->
    <link name="${prefix}tool0">
      <visual>
        <origin xyz="0 0 -0.126" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/visual/link_7.dae" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 -0.126" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://iiwa/meshes/iiwa_r820/collision/link_7.stl" />
        </geometry>
      </collision>
    </link>

    <!-- <link name="${prefix}tool0" />  -->
    <!-- end of link list -->



    <!-- ################################################################# -->
    <!-- joint list -->
    <joint name="${prefix}iiwa_j1" type="revolute">
      <origin xyz="0 0 0.36" rpy="1.5708 0 0" />
      <parent link="${prefix}iiwa_base_link" />
      <child link="${prefix}iiwa_l1" />
      <axis xyz="0 1 0" />
      <limit lower="-2.9668" upper="2.9668" effort="10" velocity="1.4834" />
    </joint>

    <joint name="${prefix}iiwa_j2" type="revolute">
      <origin xyz="-0.00043624 0 0" rpy="-1.5708 0 0" />
      <parent link="${prefix}iiwa_l1" />
      <child link="${prefix}iiwa_l2" />
      <axis xyz="0 -1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="9" velocity="1.4834" />
    </joint>

    <joint name="${prefix}iiwa_j3" type="revolute">
      <origin xyz="0 0 0.42" rpy="1.5708 0 0" />
      <parent link="${prefix}iiwa_l2" />
      <child link="${prefix}iiwa_l3" />
      <axis xyz="0 1 0" />
      <limit lower="-2.9668" upper="2.9668" effort="7" velocity="1.7452" />
    </joint>

    <joint name="${prefix}iiwa_j4" type="revolute">
      <origin xyz="0.00043624 0 0" rpy="-1.5708 0 0" />
      <parent link="${prefix}iiwa_l3" />
      <child link="${prefix}iiwa_l4" />
      <axis xyz="0 -1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="6" velocity="1.3089" />
    </joint>

    <joint name="${prefix}iiwa_j5" type="revolute">
      <origin xyz="0 0 0.4" rpy="1.5708 0 0" />
      <parent link="${prefix}iiwa_l4" />
      <child link="${prefix}iiwa_l5" />
      <axis xyz="0 1 0" />
      <limit lower="-2.9668" upper="2.9668" effort="5" velocity="2.2688" />
    </joint>

    <joint name="${prefix}iiwa_j6" type="revolute">
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <parent link="${prefix}iiwa_l5" />
      <child link="${prefix}iiwa_l6" />
      <axis xyz="0 -1 0" />
      <limit lower="-2.0942" upper="2.0942" effort="4" velocity="2.356" />
    </joint>

    <joint name="${prefix}iiwa_j7" type="revolute">
      <origin xyz="0 0 0.126" rpy="0 0 0" />
      <parent link="${prefix}iiwa_l6" />
      <child link="${prefix}tool0" />
      <axis xyz="0 0 1" />
      <limit lower="-3.0541" upper="3.0541" effort="1" velocity="2.356" />
    </joint>
    <!--
    <joint name="${prefix}iiwa_j7-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="${prefix}iiwa_l7" />
      <child link="${prefix}tool0" />
      <axis xyz="0 0 0" />
    </joint> -->
    <!-- end of joint list -->

    <!-- ROS iiwa_base_link to KUKA $ROBROOT coordinate system transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}iiwa_base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}iiwa_base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <link name="${prefix}base_link" />
    <joint name="${prefix}base_link-iiwa_base_link" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}iiwa_base_link"/>
      <child link="${prefix}base_link"/>
    </joint>

  </xacro:macro>
</robot>
